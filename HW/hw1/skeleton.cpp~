#include <cstdlib>
#include <cstdio>
#include <cstring>
#include <time.h>
#include <math.h>
#include "counters.h"
#include "sum.cpp"
#include "random.cpp"

int main(int argc, char *argv[])
{
    //Initialize a hardware counter
    hwCounter_t c1;
    c1.init = false;
    initTicks(c1);

    //Get current time in cycles
    uint64_t time = getTicks(c1);
    
    //1.3.a int s = sumA(10000);
    //1.3.b int s = sumB();
    int p = randIntArray(10, (1 << 20));
    
    uint64_t elapsed = getTicks(c1) - time;
    printf("Current Time in Ticks: %lu \n", elapsed);
    printf("Final pointer: %d \n", p);
}
